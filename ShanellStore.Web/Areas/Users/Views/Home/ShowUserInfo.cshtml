@model ShanellStore.DataLayer.Entities.Orders.UserOrder
@{
    ViewData["Title"] = "ShowUserInfo";
    Layout = "~/Areas/Users/Views/Shared/_UserLayout.cshtml";
}

<div class="col-md-6">
    <div class="form-group">
        <label for="disabledSelect">نام</label>
        <input class="form-control" id="disabledInput" type="text" placeholder="@Model.Name" disabled="">
    </div>
    <div class="form-group">
        <label for="disabledSelect">نام خانوادگی</label>
        <input class="form-control" id="disabledInput" type="text" placeholder="@Model.LastName" disabled="">
    </div>
    <div class="form-group">
        <label for="disabledSelect">شماره همراه</label>
        <input class="form-control" id="disabledInput" type="text" placeholder="@Model.Phone" disabled="">
    </div>
    <div class="form-group">
        <label for="disabledSelect">کد پستی</label>
        <input class="form-control" id="disabledInput" type="text" placeholder="@Model.PostCode" disabled="">
    </div>
    <div class="form-group">
        <label for="disabledSelect">آدرس</label>
        <input class="form-control" id="disabledInput" type="text" placeholder="@Model.Address" disabled="">
    </div>
    <div>
        <a href="/Users/Home/EditInformation/@Model.UO_Id" class="btn btn-warning">ویرایش اطلاعات</a>
    </div>
</div>

